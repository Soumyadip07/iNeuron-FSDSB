    "#Print all pronic numbers between 1 to 100\n",
    "import math\n",
    "l=[]\n",
    "for i in range(101):\n",
    "    l.append(i)\n",
    "for n in l:\n",
    "    s=int(math.sqrt(n))\n",
    "    i=0\n",
    "    while(i<=s):\n",
    "        if(n==i*(i+1)):\n",
    "            print(n,end=\",\")\n",
    "        i+=1  "
   ]
  }
