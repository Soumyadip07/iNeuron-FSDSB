    "print(\"The disarium numbers present between 1 to 100 are:-\")\n",
    "for i in range(1,100):\n",
    "    x1=i\n",
    "    l=len(str(i))\n",
    "    s=0\n",
    "    while (i>0):\n",
    "        r=i%10\n",
    "        s=s+(r**l)\n",
    "        l=l-1\n",
    "        i=i//10\n",
    "    if(s==x1):\n",
    "        print(s,end=\",\")"
   ]
